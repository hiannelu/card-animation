      loop: true,\n          centeredSlides: true,\n          watchSlidesProgress: true,\n          spaceBetween: 0,\n          slideActiveClass: 'swiper-slide-active',\n          effect: 'coverflow',\n          coverflowEffect: {\n            rotate: 0,\n            stretch: 50,\n            depth: 400,\n            modifier: 1,\n            slideShadows: false\n          }\n        },\n      books: []\n    }\n  },\n  created() {\n    this.axios.get(bookListAPI).then(response => {\n      console.log(response);\n      this.books = response.data;\n      console.log('books: ' + this.books)\n    })\n  },\n  methods: {\n    bookDetail(id) {\n      console.log(id)\n      this.$router.push(`/books/${id}`);\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .wrap-book-list {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n  }\n</style>\n"],"sourceRoot":""}]}