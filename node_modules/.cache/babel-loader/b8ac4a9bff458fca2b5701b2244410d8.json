 navigation\n      pagination\n    >\n        <swiper-slide v-for=\"(item, index) in books\" :key=\"index\" class=\"wrap-slide\">\n          <div :class=\"['card', 'shadow', 'card-book', 'animate__animated', {'animate__fadeInTopLeft':fade}]\" :style=\"{'animation-delay': index * 0.15 + 's'}\" \n            @click=\"bookDetail(item.id, $event)\"\n          >\n            <picture>\n              <source class=\"card-img-top\" type=\"image/webp\" :srcset=\"item.image\" alt=\"Book's image\">\n              <img class=\"card-img-top\" :src=\"item.image\" alt=\"Book's image\">\n            </picture>\n            <div class=\"card-body\">\n              <div class=\"card-text\">\n                {{ item.name }}\n              </div>\n            </div>\n          </div>\n        </swiper-slide>\n    </swiper>\n    </div> -->\n    \n\n  <div id=\"pc\">\n    <div class=\"wrap\">\n      <i :class=\"['bi', 'arrow', {'bi-arrow-left-circle':!mouseLeft}, {'bi-arrow-left-circle-fill':mouseLeft}]\"\n        @mouseover=\"mouseLeft=true\"\n        @mouseleave=\"mouseLeft=false\"\n        @click=\"left\"\n      >\n      </i>\n      <div class=\"slider-show\">\n        <div class=\"book-list\">\n          <div v-for=\"(item, index) in books\" :key=\"index\" class=\"wrap-slide\">\n            <div :class=\"['card', 'shadow', 'card-book', 'animate__animated', {'animate__fadeInTopLeft':fade}]\" :style=\"{'animation-delay': index * 0.15 + 's'}\" \n              @click=\"bookDetail(item.id, $event)\"\n            >\n              <picture>\n                <source class=\"card-img-top\" type=\"image/webp\" :srcset=\"item.image\" alt=\"Book's image\">\n                <img class=\"card-img-top\" :src=\"item.image\" alt=\"Book's image\">\n              </picture>\n              <div class=\"card-body\">\n                <div class=\"card-text\">\n                  {{ item.name }}\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n      <i :class=\"['bi', 'arrow', {'bi-arrow-right-circle':!mouseRight}, {'bi-arrow-right-circle-fill':mouseRight}]\"\n        @mouseover=\"mouseRight=true\"\n        @mouseleave=\"mouseRight=false\"\n        @click=\"right\"\n      >\n      </i>\n    </div>\n    <div v-if=\"test\" id=\"test\" class=\"modal-dialog\">test</div>\n    <router-link name=\"book-detail\" to=\"/books/:id\" />\n    <router-view />\n    <!-- <BookDetail /> -->\n  </div>\n  \n  <div id=\"mobile\">\n    <div>\n        <div class=\"book-list-m\">\n          <div v-for=\"(item, index) in books\" :key=\"index\" :id=\"index\" class=\"wrap-slide-m\">\n\n            <div :class=\"['card', 'shadow', 'card-book', 'animate__animated', {'animate__fadeInTopLeft':fade}]\" :style=\"{'animation-delay': index * 0.15 + 's'}\" \n              @click=\"bookDetail(item.id, $event, index)\"\n            >\n              <picture>\n                <source class=\"card-img-top\" type=\"image/webp\" :srcset=\"item.image\" alt=\"Book's image\">\n                <img class=\"card-img-top\" :src=\"item.image\" alt=\"Book's image\">\n              </picture>\n              <div class=\"card-body\">\n                <div class=\"card-text\">\n                  {{ item.name }}\n                </div>\n              </div>\n            </div>\n\n            <div style=\"margin-top:3vh\">\n            <router-link name=\"book-detail\" :to=\"{path:'/books/' + item.id}\" />\n            <router-view :id=\"['view' + index]\" v-if=\"index == selectedId\" />\n            </div>\n\n          </div>\n           \n        </div>\n    </div>\n  </div>\n\n\n    \n    \n  </div>\n</template>\n\n<script>\n\nconst bookListAPI = 'https://fe-interview-api.unnotech.com/books';\n// const bookDetailAPI = 'https://fe-interview-api.unnotech.com/profile/';\n// import { Swiper, SwiperSlide } from 'swiper/vue';\n// import SwiperCore, { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\n// import 'swiper/components/navigation/navigation.scss';\n// import 'swiper/components/pagination/pagination.scss';\n\n// SwiperCore.use([Navigation, Pagination, Scrollbar, A11y]);\n// import $ from 'jquery';\n// import BookDetail from '@/components/BookDetail.vue';\n\nexport default {\n  components: {\n    // Swiper,\n    // SwiperSlide\n    // BookDetail\n  },\n  data() {\n    return {\n      books: [],\n      fade: true,\n      mouseLeft: '',\n      mouseRight: '',\n      lastIndex: '',\n      detail: '',\n      selectedId: '',\n      test: ''\n    }\n  },\n  mounted() {\n    this.axios.get(bookListAPI).then(response => {\n      this.books = response.data;\n    }).then(() => {\n        document.querySelector('.card').addEventListener('animationend', () => {\n          this.fade = false;\n        });\n    })\n  },\n  methods: {\n    bookDetail(itemId, event, index) {\n      const current = event.currentTarget;\n      const selected = document.querySelectorAll('.selected-card');\n      // const detail = document.querySelector('.selected-detail');\n      // const div = document.createElement('div');\n      // const lastIndex = document.getElementById(this.lastIndex);\n      \n      // const currIndex = document.getElementById(index);\n\n      // if (detail != null) {\n\n      //   lastIndex.removeChild(detail);\n        \n      // }\n      this.selectedId = index;\n      Array.prototype.slice.call(selected).forEach(card => {\n          card.classList.remove('selected-card');\n          \n        })\n\n      current.classList.add('flip', 'selected-card');\n      // div.classList.add('selected-detail');\n      // div.innerHTML = `\n      //   <p>hiiiii</p>\n      // `;\n      // currIndex.appendChild(div);\n      // this.lastIndex = index;\n\n      \n\n\n      \n      current.addEventListener('animationend', () => {\n        current.classList.remove('flip');\n      });\n      this.$router.push(`/books/${itemId}`);\n      \n    },\n    // getDetail() {\n    //   const element = document.querySelector('#btn');\n\n    //   if (this.detail != '') {\n    //     this.fade = 'animate__lightSpeedOutRight';\n    //     element.addEventListener('animationend', () => {\n    //       this.details();\n    //     });\n    //   } else {\n    //     this.axios.get(`${bookDetailAPI}${this.$route.params.id}`).then(response => {\n    //     console.log(response);\n    //     this.detail = {\n    //       price: parseInt(response.data.price, 10),\n    //       count: parseInt(response.data.count, 10)\n    //     }\n    //   }).then(() => this.fade = 'animate__lightSpeedInLeft')\n    //   }\n    // },\n    left() {\n      const bookList = document.querySelector('.book-list');\n      let deg = document.querySelector('body').style.getPropertyValue('--deg');\n      console.log(typeof(deg), deg)\n\n      if (deg == '' || deg == '0%') {\n        deg = 0;\n        console.log('third')\n      }\n      else {\n        deg.replace('/%/', '');\n        deg = parseInt(deg, 10) + 32;\n        console.log('fourth')\n      }\n\n      bookList.classList.add('slide-move');\n      document.querySelector('body').style.setProperty('--deg', deg + '%');\n      \n      \n    },\n    right() {\n       const bookList = document.querySelector('.book-list');\n      let deg = document.querySelector('body').style.getPropertyValue('--deg');\n      console.log('1' + typeof(deg), deg)\n\n      if (deg == '' || deg == '0%') {\n        deg = -32;\n        console.log('first')\n      } else if (deg == '-96%') {\n        deg = -96;\n      } else {\n        deg.replace('/%/', '');\n        deg = parseInt(deg, 10) - 32;\n        console.log('second')\n      }\n\n      bookList.classList.add('slide-move');\n      document.querySelector('body').style.setProperty('--deg', deg + '%');\n      console.log('2' + typeof(deg), deg)\n      // document.querySelector('#test').classList.add('modal-dialog')\n      this.test = true;\n    }\n  }\n}\n</script>\n\n<style scoped>\n  /* .wrap-book-list {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n  } */\n  .modal-dialog{\n   transform: translate(200px, 0);\n    transition: 1s;\n}\n  .wrap {\n    display: flex;\n    flex-direction: row;\n    background: green;\n    justify-content: center;\n    width: 85%;\n    margin: 0 auto;\n    \n  }\n  .arrow {\n    font-size: 80px;\n    display: flex;\n    align-items: center;\n    /* justify-content: center; */\n    /* background: red; */\n  }\n  .slider-show {\n    width: 84%;\n    /* width: 1038px; */\n    overflow: hidden;\n    background: yellow;\n    cursor: grab;\n  }\n  .book-list {\n    margin: 0 auto;\n    display: grid;\n    grid-auto-flow: column;\n    /* overflow: hidden; */\n    background: red;\n    \n  }\n\n  .wrap-slide {\n    padding-left: 5vw;\n    /* padding-left: 22%; */\n    margin: 5vh 0 5vh 0;\n  }\n\n  .card-book img {\n    height: 35vh;\n  }\n\n  .card-book {\n    height: 50vh;\n    /* width: 16vw; */\n    width: 250px;\n    margin: 0 auto;\n  }\n\n  .card-book:hover {\n    transform: scale(1.1);\n  }\n\n  .flip {\n    animation: flipInY;\n    animation-duration: 1s;\n  }\n\n  .fade {\n    animation: fadeInTopLeft;\n    animation-duration: 1s;\n  }\n\n  body {\n    --deg: 0;\n  }\n\n  .slide-move {\n    /* transform: translate(-20%, 0); */\n    transform: translate(var(--deg), 0);\n    transition: 1s;\n  }\n  /* #left:checked ~ .book-list {\n    transform: translate(-20%, 0);\n    transition: 1s;\n  } */\n\n  /* @keyframes left {\n    100% {\n      transform: translate(-20%, 0);\n    }\n  } */\n\n  .book-list-m {\n    background: yellow;\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n    margin-top: 5vh;\n    /* padding-top: 5vh; */\n  }\n\n  #mobile {\n    display: none;\n  }\n\n  #pc {\n    display: inline-block;\n    width: 100%;\n  }\n\n  @media (max-width: 546px) {\n    #mobile {\n      display: inline-block;\n      width: 100%;\n    }\n    #pc {\n      display: none;\n    }\n  }\n\n  .selected-card {\n    /* width: 300px;\n    height: 55vh;\n    background: chocolate; */\n    /* margin-top: 5vh; */\n    border: 5px #007bff solid;\n    border-radius: 5px;\n\n  }\n\n  .wrap-slide-m {\n    margin-top: 5vh;\n    background: red;\n  }\n\n\n</style>\n"],"sourceRoot":""}]}