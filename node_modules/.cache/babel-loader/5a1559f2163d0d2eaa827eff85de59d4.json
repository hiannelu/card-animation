> {\n      console.log(response);\n      this.books = response.data;\n      console.log('books: ' + this.books)\n    })\n  },\n  methods: {\n    bookDetail(id) {\n      console.log(id)\n      this.$router.push(`/books/${id}`);\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .wrap-book-list {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n  }\n</style>\n"],"sourceRoot":""}]}