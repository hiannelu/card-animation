AC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/anne/vue_unnotech/src/components/Books.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <swiper\n      :slides-per-view=\"3\"\n      navigation\n      pagination\n    >\n        <swiper-slide v-for=\"(item, index) in books\" :key=\"index\" class=\"wrap-slide\">\n          <div :class=\"['card', 'shadow', 'card-book', 'animate__animated', {'animate__fadeInTopLeft':fade}]\" :style=\"{'animation-delay': index * 0.15 + 's'}\" \n            @click=\"bookDetail(item.id, $event)\"\n          >\n            <picture>\n              <source class=\"card-img-top\" type=\"image/webp\" :srcset=\"item.image\" alt=\"Book's image\">\n              <img class=\"card-img-top\" :src=\"item.image\" alt=\"Book's image\">\n            </picture>\n            <div class=\"card-body\">\n              <div class=\"card-text\">\n                {{ item.name }}\n              </div>\n            </div>\n          </div>\n        </swiper-slide>\n    </swiper>\n\n    <router-link name=\"book-detail\" to=\"/books/:id\" />\n    <router-view />\n  </div>\n</template>\n\n<script>\n\nconst bookListAPI = 'https://fe-interview-api.unnotech.com/books';\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport SwiperCore, { Navigation, Pagination, Scrollbar, A11y } from 'swiper';\nimport 'swiper/components/navigation/navigation.scss';\nimport 'swiper/components/pagination/pagination.scss';\n\nSwiperCore.use([Navigation, Pagination, Scrollbar, A11y]);\n\nexport default {\n  components: {\n    Swiper,\n    SwiperSlide\n  },\n  data() {\n    return {\n      books: [],\n      fade: true\n    }\n  },\n  mounted() {\n    this.axios.get(bookListAPI).then(response => {\n      this.books = response.data;\n    }).then(() => {\n        document.querySelector('.card').addEventListener('animationend', () => {\n          this.fade = false;\n        });\n    })\n  },\n  methods: {\n    bookDetail(id, event) {\n      const current = event.currentTarget;\n\n      current.classList.add('flip');\n      current.addEventListener('animationend', () => {\n        current.classList.remove('flip');\n      });\n      this.$router.push(`/books/${id}`);\n    }\n  }\n}\n</script>\n\n<style scoped>\n  /* .wrap-book-list {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n  } */\n\n  .wrap-slide {\n    padding-left: 6vw;\n    margin: 5vh 0 5vh 0;\n  }\n\n  .card-book img {\n    height: 35vh;\n  }\n\n  .card-book {\n    height: 50vh;\n    width: 16vw;\n  }\n\n  .card-book:hover {\n    transform: scale(1.1);\n  }\n\n  .flip {\n    animation: flipInY;\n    animation-duration: 1s;\n  }\n\n  .fade {\n    animation: fadeInTopLeft;\n    animation-duration: 1s;\n  }\n\n\n</style>\n"]}]}