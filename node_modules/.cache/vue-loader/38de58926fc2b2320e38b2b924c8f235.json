AE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;UAEvC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;;UAGjC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;UAClG,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,EAAE;UACpB,CAAC,EAAE,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC9C,CAAC,EAAE,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,EAAE;UACb,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC5C,CAAC,EAAE;;;;QAIL;MACF,CAAC;IACH,CAAC;EACH;AACF","file":"/Users/anne/vue_unnotech/src/components/BookDetail.vue","sourceRoot":"","sourcesContent":["<template>\n\n  <div>\n    \n  <div @click=\"closeModal\">\n    <div class=\"wrap-book-detail\">\n\n      <table align=\"center\">\n        <tbody>\n          <tr :class=\"['animate__animated', fade]\">\n            <td><label for=\"inputPrice\">價格</label></td>\n            <td class=\"input-group mb-3\">\n              <div class=\"input-group-prepend\">\n                <button class=\"btn\"><i class=\"bi bi-dash-square\" @click=\"minusPrice\"></i></button>\n                <span class=\"input-group-text\">NTD $</span>\n              </div>\n              <input id=\"inputPrice\" \n                :class=\"['needs-validation', {'is-invalid':validation[0]}, 'form-control']\" \n             \n                v-model=\"detail.price\"\n              >\n              <div class=\"invalid-feedback\">Please enter positive number.</div>\n              <div class=\"input-group-append\">\n                <button class=\"btn\"><i class=\"bi bi-plus-square\" @click=\"addPrice\"></i></button>\n              </div>\n            </td>\n          </tr>\n          <tr :class=\"['animate__animated', fade, 'delay', 'animate__delay-1s']\">\n            <td><label for=\"inputCount\">數量</label></td>\n            <td class=\"input-group mb-3\">\n              <div class=\"input-group-prepend\">\n                <button class=\"btn\"><i class=\"bi bi-dash-square\" @click=\"minusCount\"></i></button>\n              </div>\n              <input id=\"inputCount\" \n                :class=\"['needs-validation', {'is-invalid':validation[1]}, 'form-control']\" \n                style=\"padding-left:5.5rem\"\n                v-model=\"detail.count\"\n              >\n              <div class=\"invalid-feedback\">Please enter positive number.</div>\n              <div class=\"input-group-append\">\n                <button class=\"btn\"><i class=\"bi bi-plus-square\" @click=\"addCount\"></i></button>\n              </div>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n\n      <button id=\"btn\" :class=\"['btn', 'btn-primary', 'animate__animated', fade, 'delay', 'animate__delay-2s']\" @click=\"submit\">\n        確認修改\n        <i class=\"bi bi-pencil-square\"></i>\n      </button>\n    </div>\n  </div>\n  <div class=\"modal-dialog\">test</div>\n<!-- \n<div class=\"flip-detail\">\n  detail\n  <div class=\"flip\">\n    <div class=\"front\">front</div>\n    \n    <div class=\"back\">back</div>\n  </div>\n</div> -->\n\n<!-- <div class=\"popup-modal\">\n    <div class=\"modal fade display:inline-block\" id=\"modal\" tabindex=\"-1\" role=\"dialog\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\n      <div class=\"modal-dialog\" role=\"document\">\n        <div class=\"modal-content\">\n          <div class=\"modal-body\">\n            <i class=\"bi bi-check-circle\"></i>\n            Successfully updated!\n          </div>\n        </div>\n      </div>\n    </div>\n</div> -->\n\n<div style=\"style\">123</div>\n\n    <div class=\"open-modal\">\n    <div class=\"modal-dialog\">\n      <i class=\"bi bi-check-circle\"></i>\n      Successfully updated!\n    </div>\n  </div>\n\n\n\n\n\n<!-- <transition name=\"slide-fade\">\n  <div v-if=\"modal\" class=\"open-modal\">\n    <div class=\"modal-dialog\">\n      <i class=\"bi bi-check-circle\"></i>\n      Successfully updated!\n    </div>\n  </div>\n</transition> -->\n\n\n</div>\n\n</template>\n\n<script>\n\n// import $ from 'jquery';\n\nconst bookDetailAPI = 'https://fe-interview-api.unnotech.com/profile/';\n\nexport default {\n  data() {\n    return {\n      detail: '',\n      \n      fade: 'animate__lightSpeedInLeft',\n      validation: [false, false],\n      modal: '',\n\n      show: false,\n      style: {\n        animationName: \"popup\",\n          animationIterationCount: \"infinite\",\n          animationDuration: \"4s\",\n          animationTimingFunction: \"ease-in-out\",\n          animationDirection: \"alternate\",\n          animationFillMode: \"both\",\n          animationPlayState: \"running\"\n      }\n    }\n  },\n  created() {\n    \n  },\n  mounted() {\n    this.getDetail();\n  },\n  watch: {\n    $route() {\n      this.getDetail();\n    }\n  },\n  methods: {\n    getDetail() {   \n      const element = document.querySelector('#btn');\n\n      if (this.detail != '') {\n        this.fade = 'animate__lightSpeedOutRight';\n        element.addEventListener('animationend', () => {\n          this.details();\n        });\n      } else this.details();\n    },\n    details() {\n      this.axios.get(`${bookDetailAPI}${this.$route.params.id}`).then(response => {\n        console.log(response);\n        this.detail = {\n          price: parseInt(response.data.price, 10),\n          count: parseInt(response.data.count, 10)\n        }\n      }).then(() => this.fade = 'animate__lightSpeedInLeft')\n    },\n    addPrice() {\n      this.detail.price ++;\n    },\n    minusPrice() {\n      if (this.detail.price > 0) this.detail.price --;\n    },\n    addCount() {\n      this.detail.count ++;\n    },\n    minusCount() {\n      if (this.detail.count > 0) this.detail.count --;\n    },\n    submit() {\n      const vm = this;\n      const forms = document.querySelectorAll('.needs-validation');\n\n      Array.prototype.slice.call(forms).forEach(function (form, index) {\n        if (isNaN(form.value) || form.value == '' || form.value <  0) {\n          vm.validation.splice(index, 1, true);\n        } else {\n          vm.validation.splice(index, 1, false);\n        }\n      })\n      if (this.validation.every(e => e == false)) this.updateDetail();\n    },\n    updateDetail() {\n      // const modal = document.getElementById('modal');\n      // const vm = this;\n      const top = document.documentElement.scrollTop;\n      // const dialog = document.querySelector('.modal-dialog');\n      // const width = document.documentElement.clientWidth;\n\n      this.axios.patch(`${bookDetailAPI}${this.$route.params.id}`, this.detail).then(response => {\n        console.log(response)\n        if (response.status == 200) {\n          // $('#modal').modal('show');\n\n          this.modal = true;\n\n          // setTimeout(() => {\n          //   vm.modal = false\n          // }, 500); \n\n          // document.querySelector('body').classList.add('fixed-position');\n          // document.body.className = 'fixed-position'\n          console.log(document.documentElement.scrollTop)\n          document.querySelector('body').style.setProperty('--top', top + 'px');\n          console.log(document.documentElement.clientWidth)\n          document.body.style.overflow = \"hidden\"; \n          \n          // dialog.classList.add('pop-up');\n\n\n          // if(/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {\n          // if (width < 546) {\n          // //   document.body.style.position = 'fixed';\n          // // } else {\n          //   document.body.style.overflow = \"hidden\"; \n          // }\n          \n          \n          \n        }\n      })\n    },\n  }\n}\n</script>\n\n<style scoped>\n\nbody {\n  --top: 0;\n}\n/* \n.fade-enter-active,\n.fade-leave-active {\n  transition: opacity 1s\n}\n\n.fade-enter,\n.fade-leave-to\n\n\n{\n  opacity: 0\n} */\n\n\n@-webkit-keyframes popup {\n  0%, 100% {\n    opacity: 0;\n  }\n  50% {\n    opacity: 1;\n  }\n}\n\n.wrap-book-detail {\n  width: 30%;\n  margin: 0 auto;\n  padding: 3vh 1vh 2vh 1vh;\n  background: #FEAC5E;\n  background: -webkit-linear-gradient(to right, #4BC0C8, #C779D0, #FEAC5E);\n  background: linear-gradient(to right, #4BC0C8, #C779D0, #FEAC5E);\n}\n\n\n.delay {\n  --animate-delay: 0.1s\n}\n\n\n/* .modal-dialog {\n  display: inline-block;\n  height: 80%;\n  width: 15rem;\n  display:flex;\n  align-items:center;\n} */\n.modal-dialog{\n  background: white;\n  display: inline-block;\n  padding: 15px;\n  border-radius: 5px;\n  /* transform: translate(0, 300px);\n  transition: 1s; */\n}\n\n\n.open-modal {\n  width: 100%;\n  height: 100%;\n  background: rgb(0, 0, 0, .4);\n  position: absolute;\n  top: var(--top);\n  left: 0;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n\n\n.flip-detail {\n  width: 20%;\n  height: 100px;\n  position: relative;\n  perspective: 1500px;\n  background-color: blue;\n  margin: 0 auto;\n}\n\n.flip {\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  transform-origin: left;\n  transform-style: preserve-3d;\n  /* transform: rotateY(0deg); */\n  transition: .5s;\n  background-color: yellow;\n  backface-visibility: hidden;\n}\n.flip:hover{\n  transform: rotateY(-180deg);\n}\n\n.front {\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  background-color: red;\n  backface-visibility: hidden;\n\n}\n\n.back {\n  width: 100%;\n  height: 100%;\n  position: absolute;\n  top: 0;\n  left: 0;\n  /* z-index: 99; */\n  transform: rotateY(180deg);\n  /* backface-visibility: hidden; */\n  background-color: green;\n}\n\n\n  @media (max-width: 546px) {\n    .wrap-book-detail {\n      width: 100%;\n      margin: 0 auto;\n      padding: 3vh 1vh 2vh 1vh;\n      background: #FEAC5E;\n      background: -webkit-linear-gradient(to right, #4BC0C8, #C779D0, #FEAC5E);\n      background: linear-gradient(to right, #4BC0C8, #C779D0, #FEAC5E);\n    }\n  }\n\n</style>\n"]}]}